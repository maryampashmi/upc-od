[
  {
    "name": "Amazon-Crawling",
    "adapter": {
      "name": "crawler",
      "config": {
        "baseUrl": "http://www.amazon.org/",
        "pathPattern": "products/:title",
        "select": "#offer > .price"
      }
    }
  },
  {
    "name": "Wikipedia-Crawling",
    "adapter": {
      "name": "crawler",
      "config": {
        "baseUrl": "http://en.wikipedia.org/",
        "pathPattern": "wiki/:title",
        "select": "#offer > .price"
      }
    }
  },
  {
    "name": "Wikidata",
    "adapter": {
      "name": "wikidata",
      "config": {
      }
    }
  },
  {
    "name": "DBpedia",
    "adapter": {
      "name": "sparql",
      "config": {
        "endpoint": "http://www.example.com/"
      }
    }
  },
  {
    "name": "Freebase",
    "adapter": {
      "name": "sparql",
      "config": {
        "endpoint": "http://www.example.com/freebase/sparql"
      }
    }
  }
]
